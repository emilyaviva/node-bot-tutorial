{
   "settings": {
    "ecmascript": 5
  },
  "ecmaFeatures": {
  },
  "env": {
    "node": true
  },
  "rules": {
    "semi": 1,
    "strict": 0,
    "indent": [2, 2],
    "quotes": [1, "single"],
    "no-multi-spaces": [1, {
      "exceptions": {
        "VariableDeclarator": true,
        "FunctionExpression": true
      }
    }],
    "key-spacing": [0, {"align": "value"}],
    "no-underscore-dangle": 0
  }
}
